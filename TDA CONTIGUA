public class TDAcontigua {
    
    int[] lista;
    int elemento = 0;
    
    
    public void adicionarFInal (int dato) {
        lista [elemento] = dato;
        elemento ++ ;
      
    }
    public void borrarFinal(){
        elemento -- ;
        
    }
    public void mostrar (){
        for (int i = 0; i < elemento; i++) {
            System.out.println(lista[i]); 
            
        }
    }
            
}
